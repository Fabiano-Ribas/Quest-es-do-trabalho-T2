#include <stdio.h>
#include <stdlib.h>
int soma (int x, int y);
int main()
{
    int x, y, resultado;

    printf("Informe a primeria parcela: ");
    scanf("%d", & x);
    printf("Informe a segunda parcela: ");
    scanf("%d", & y);
    resultado= soma(x,y);
    printf("O resultado eh: %d\n", resultado);

    return 0;
}

int soma(int x, int y){

    int resultado = x+y;

    return resultado;

}
